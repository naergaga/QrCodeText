<Window x:Class="QrCodeText.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QrCodeText"
        mc:Ignorable="d"
        Title="QrCodeText" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <StackPanel Margin="5" Grid.ColumnSpan="2" Orientation="Horizontal">
            <Button Padding="10 3" Click="Button_Click">导入</Button>
        </StackPanel>
        <Image Name="Image1" Margin="5" Grid.Row="2" Grid.Column="0">
            
        </Image>
        <TextBox Name="Text1" VerticalScrollBarVisibility="Auto" TextWrapping="Wrap" Margin="5" Grid.Row="2" Grid.Column="1">

        </TextBox>
    </Grid>
</Window>
